<?xml version="1.0" encoding="UTF-8"?>
<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:section title="${%Agents On Demand}">
        <f:entry field="enabled" title="${%Enabled}">
            <f:checkbox/>
        </f:entry>
        <f:entry field="excludeLabels" title="${%ExcludeLabels}">
            <f:expandableTextbox/>
        </f:entry>
        <f:entry field="agentProvisionJob" title="${%AgentProvisionJob}">
            <f:textbox/>
        </f:entry>
        <f:entry field="agentDecomissionJob" title="${%AgentDecomissionJob}">
            <f:textbox/>
        </f:entry>
        <f:advanced>
             <f:entry field="revision" title="${%Revision}">
            <f:textbox/>
            </f:entry>
            <f:entry field="gitUrl" title="${%GitUrl}">
                <f:textbox/>
            </f:entry>
            <f:entry field="gitRevision" title="${%GitRevision}">
                <f:textbox/>
            </f:entry>
            <f:entry field="agentImage" title="${%AgentImage}">
                <f:textbox/>
            </f:entry>
            <f:entry field="agentWorkDir" title="${%AgentWorkDir}">
                <f:textbox/>
            </f:entry>
            <f:entry field="maxNodesPerLabel" title="${%MaxNodesPerLabel}">
                <f:number/>
            </f:entry>
            <f:entry field="maxIdleTime" title="${%MaxIdleTime}">
                <f:number/>
            </f:entry>
        </f:advanced>
    </f:section>
</j:jelly>
